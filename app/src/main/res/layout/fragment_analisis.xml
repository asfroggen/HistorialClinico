<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MenuPaciente.Analisis.AnalisisFragment"
    android:background="@color/colorFondoClaro"
    android:orientation="vertical">

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tlAnalisis"
        app:tabBackground="@color/colorOrangeFuerte"
        app:tabTextColor="@color/colorFondoLogin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <androidx.viewpager.widget.ViewPager
        android:id="@+id/vpTabsAnalisis"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>